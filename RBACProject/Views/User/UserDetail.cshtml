@model RBACProject.Model.UserModelResult
@{
    ViewBag.Title = "Detail";
    Layout = "~/Views/Shared/_LayoutDetail.cshtml";
}

<div class="layui-col-md12">
    <table class="layui-table" lay-skin="line">
        <tbody>
            <tr>
                <td>@Html.DisplayNameFor(x => x.UserName)：</td>
                <td>@Model.UserName</td>
                <td>@Html.DisplayNameFor(x => x.RealName)：</td>
                <td>@Model.RealName</td>
            </tr>
            <tr>
                <td>@Html.DisplayNameFor(x => x.Status)：</td>
                <td>@Html.DisplayStatusHtml(Model.Status)</td>
                <td>@Html.DisplayNameFor(x => x.Gender)：</td>
                <td>@Html.DisplayGenderHtml(Model.Gender)</td>
            </tr>

            <tr>
                <td>@Html.DisplayNameFor(x => x.Phone)：</td>
                <td>@Html.DisplayFor(x => x.Phone)</td>
                <td>@Html.DisplayNameFor(x => x.Email)：</td>
                <td>@Html.DisplayFor(x => x.Email)</td>
            </tr>

            <tr>
                <td>@Html.DisplayNameFor(x => x.DeptId)：</td>
                <td>@Html.DisplayFor(x => x.deptName)</td>
                <td>@Html.DisplayNameFor(x => x.Remark)：</td>
                <td>@Html.DisplayFor(x => x.Remark)</td>
            </tr>

            <tr>
                <td>@Html.DisplayNameFor(x => x.LoginIP)：</td>
                <td>@Html.DisplayFor(x => x.LoginIP)</td>
                <td>@Html.DisplayNameFor(x => x.LoginDate)：</td>
                <td>@Html.DisplayFor(x => x.LoginDate)</td>
            </tr>

            <tr>
                <td>@Html.DisplayNameFor(x => x.CreateOn)：</td>
                <td>@Html.DisplayFor(x => x.CreateOn)</td>
                <td>@Html.DisplayNameFor(x => x.UpdateOn)：</td>
                <td>@Html.DisplayFor(x => x.UpdateOn)</td>
            </tr>
            <tr>
                <td>@Html.DisplayNameFor(x => x.CreateBy)：</td>
                <td>@Html.DisplayFor(x => x.CreateBy)</td>
                <td>@Html.DisplayNameFor(x => x.UpdateBy)：</td>
                <td>@Html.DisplayFor(x => x.UpdateBy)</td>
            </tr>
</table>
</div>
<script>
    layui.use(['form'], function () {
        var form = layui.form;
    });
</script>

